{
  "name": "text-marker",
  "version": "1.0.0",
  "description": "A lexical analyzer for parsing text",
  "main": "index.js",
  "scripts": {
    "postinstall": "mkdir -p dist/",
    "start": "nodemon --delay 1.5 index",
    "test": "jest src/ --coverage",
    "compile": "rm -rf dist/* && babel src/ --out-dir dist/ ",
    "build": "npm run compile && webpack",
    "build:prod": "npm run compile && webpack --config ./webpack.prod.config.js",
    "dev": "npm run build && npm run webpack:watch & babel src/ --out-dir dist/ --watch",
    "webpack:watch": "webpack --watch",
    "lint": "eslint src/**/*.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "express": "^4.15.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2"
  }
}
